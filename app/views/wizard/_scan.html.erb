<p>Please insert following token, or scan the QR code.</p>
<pre><%= @token.as_str(@host) %></pre>
<center>
  <% qr = @token.as_qr(@host) %>
  <table class="qr-code">
    <% qr.modules.each_index do |x| %>
      <tr>
        <% qr.modules.each_index do |y| %>
          <% if qr.dark?(x,y) %>
            <td class="black"/>
          <% else %>
            <td class="white"/>
          <% end %>
        <% end %>
      </tr>
    <% end %>
  </table>
</center>
